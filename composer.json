{
  "name": "groton-school/slim-norms",
  "description": "Define some normal interfaces and abstract classes for working with the Slim Framework",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "autoload": {
    "psr-4": {
      "GrotonSchool\\Slim\\Norms\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Seth Battis",
      "email": "sbattis@groton.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "require": {
    "php-di/php-di": ">=6.0",
    "psr/http-message": "^2.0",
    "slim/http": "^1.4",
    "slim/slim": "^4.14"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.85",
    "marcocesarato/php-conventional-changelog": "^1.17"
  }
}
